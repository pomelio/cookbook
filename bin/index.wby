import ext.web as web;
import std.string as str;


let ppath = web.path();

if ppath == '/' || str.ends_with(ppath, '.md') {
  dispatch('/markdown', {});
} elsif str.starts_with(ppath, '/public') {
  web.send_file(ppath);
} elsif ppath == '/auth/google' {
  dispatch('/auth_google', {});
} else {
  web.set_status(404);
}
